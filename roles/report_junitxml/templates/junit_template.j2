<testsuite name="performance" tests="1" failures="{{ failures }}" errors="{{ errors }}" skipped="0" time="{{ time | default('1') }}">
    <testcase classname="performance" name="{{ dut_type }}" time="1">
        {% if perf_result_status == 'failed' %}
        <failure type="low_performance">performance are lower then expected. Expected: {{ performance_threshold }}, Got: {{ perf_results.stdout | int }}</failure>
        {% endif %}
        {% if perf_result_status == 'error' %}
        <error message="Unable to fetch results, this usually indicate a failure in the playbook">failed</error>
        {% endif %}
    </testcase>
</testsuite>
